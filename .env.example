# Environment Variables Template
# Copy this file to .env and fill in your actual values

# Vercel deployment token (optional, for automated deployments)
VERCEL_TOKEN=your_vercel_token_here

# Netlify deployment token (optional, for automated deployments)
NETLIFY_AUTH_TOKEN=your_netlify_token_here

# Google Analytics ID (optional)
GA_TRACKING_ID=your_ga_id_here

# Contact form endpoint (optional)
CONTACT_FORM_ENDPOINT=your_form_endpoint_here

# API endpoints (if needed)
API_BASE_URL=https://api.example.com

# Development settings
NODE_ENV=development
PORT=3000

# Lighthouse CI GitHub App Token (optional, for PR comments)
LHCI_GITHUB_APP_TOKEN=your_lhci_token_here